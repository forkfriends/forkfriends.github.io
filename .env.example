EXPO_PUBLIC_API_BASE_URL=http://localhost:8787
# Cloudflare Turnstile Site Key (get from https://dash.cloudflare.com/turnstile)
# Leave empty to skip Turnstile verification during development
EXPO_PUBLIC_TURNSTILE_SITE_KEY=
# Mapbox public access token (pk.*)
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=
# Web ad fallback IDs (AdSense). Leave empty to use Google test IDs by default.
# EXPO_PUBLIC_ADSENSE_CLIENT_ID=ca-pub-xxxxxxxxxxxxxxxx
# EXPO_PUBLIC_ADSENSE_SLOT_ID=1234567890
# Force placeholder ads on web while AdSense is under review (1 = placeholder)
# EXPO_PUBLIC_AD_PLACEHOLDER=1
