{
  "expo": {
    "name": "QueueUp",
    "slug": "queueup",
    "version": "1.0.0",
    "scheme": "queueup",
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "single",
      "bundler": "metro",
      "name": "QueueUp",
      "shortName": "QueueUp",
      "description": "Queue management made simple",
      "themeColor": "#222222",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "scope": "/",
      "startUrl": "/"
    },
    "experiments": {
      "tsconfigPaths": true,
      "baseUrl": "/"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo-icon.png",
          "color": "#222222",
          "sounds": []
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/logo-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.queueup.app",
      "associatedDomains": ["applinks:queueup-api.danielnwachukwu.workers.dev"],
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.queueup.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "queueup",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/logo-icon.png",
      "color": "#222222"
    },
    "extra": {
      "eas": {
        "projectId": "a570553d-0ebf-4de7-9895-b8f0e312805e"
      }
    },
    "owner": "queueup"
  }
}
